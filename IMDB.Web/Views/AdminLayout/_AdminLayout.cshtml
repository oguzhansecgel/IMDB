<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("HeadPartial.cshtml")

<body>
    <div class="container-fluid position-relative d-flex p-0">
        <!-- Spinner Start -->
        @await Html.PartialAsync("SpinnerPartial.cshtml")
        <!-- Spinner End -->
        <!-- Sidebar Start -->
        @await Html.PartialAsync("SideBarPartial.cshtml")
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            @await Html.PartialAsync("NavBarPartial.cshtml")
            <!-- Navbar End -->
            <!-- Blank Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row vh-100 bg-secondary rounded align-items-center justify-content-center mx-0">
                    @RenderBody()
                </div>
            </div>
            <!-- Blank End -->
            <!-- Footer Start -->
            @await Html.PartialAsync("FooterPartial.cshtml")
            <!-- Footer End -->
        </div>
        <!-- Content End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

   @await Html.PartialAsync("ScriptPartial.cshtml")
</body>

</html>
 